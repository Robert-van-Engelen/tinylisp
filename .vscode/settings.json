{
    "files.associations": {
        "tinylisp.h": "c",
        "functional": "c",
        "ostream": "c",
        "utility": "c"
    },
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
    "makefile.makeDirectory": "./src",
    "makefile.launchConfigurations": [
        {
            "cwd": "${workspaceFolder}/src",
            "binaryPath": "${workspaceFolder}/src/tinylisp-tested",
            "binaryArgs": []
        },
        {
            "cwd": "${workspaceFolder}/src",
            "binaryPath": "${workspaceFolder}/src/test-tl-tested",
            "binaryArgs": []
        }
    ],
    "makefile.loggingLevel": "Verbose",
    "makefile.makefilePath": "./src/Makefile"
}
